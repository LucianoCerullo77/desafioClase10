<table class="table">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Precio</th>
            <th scope="col">Photo URL</th>
        </tr>
    </thead>
    <tbody>
        <% if (datos.length === 0) { %>
            <tr>
                <td colspan="3">No hay datos</td>
            </tr>
        <% }else{ %>
        
        <% for (let i = 0; i < datos.length; i++) { %>
            <tr>
                <td><%=datos[i].nombre%></td>
                <td><%=datos[i].precio%></td>
                <td><img src="<%=datos[i].URL%>" alt=""></td>
            </tr>
        <% } } %>
    </tbody>